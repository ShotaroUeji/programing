
public class IntSet {
	int[] array;
	int num;
	
	public void init() {
		array = new int[4];
		num = 0;
	}
	public void add(int v) {
		if(num < 4 && find(v))array[num++] = v;
	}
	public boolean find(int v) {
		for(int i = 0;i < num;i++)
			if(array[i] == v)return true;
		return false;
	}
	
	public int count(){
		return array.length;
	}
	
	public void remove(int v) {
		if(find(v)) {
			
			
		}else {System.out.println("we cannot find your inputnumber");
			
		}
		
	}
	
	
	

}
